<Window x:Class="IconSwapperGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IconSwapperGui"
        xmlns:userControls="clr-namespace:IconSwapperGui.UserControls"
        Style="{StaticResource MaterialDesignWindow}"
        mc:Ignorable="d"
        Title="Icon Swapper" Height="620" Width="1050" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" UseLayoutRounding="True">
        <TabControl Grid.Column="1" Margin="5">
                <TabItem Header="Swapper">
                        <userControls:IconSwapperUserControl />
                </TabItem>
                <TabItem Header="Converter">
                        <userControls:IconConverterUserControl />
                </TabItem>
        </TabControl>
</Window>