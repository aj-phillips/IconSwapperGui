<Window x:Class="IconSwapperGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IconSwapperGui"
        xmlns:userControls="clr-namespace:IconSwapperGui.UserControls"
        mc:Ignorable="d"
        Title="Icon Swapper" Height="450" Width="800" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" UseLayoutRounding="True">
    <TabControl Margin="5">
        <TabItem Header="Icon Swapper">
            <userControls:IconSwapperUserControl />
        </TabItem>
        <TabItem Header="Icon Converter">
            <userControls:IconConverterUserControl />
        </TabItem>
    </TabControl>
</Window>